<data>
    <object id="door_basic">
        <name>door</name>
        <component id="door_link" type="link" movable="true" visible="false"/>
    </object>
    <object id="door_fake">
        <name>door</name>
    </object>
    <object id="door_locked">
        <name>door</name>
        <component id="door_link" type="link" movable="true" visible="false">
            <condition type="boolean" invert="true">
                <value type="stat" dataType="boolean" stat="locked" holder="parentObject"/>
            </condition>
        </component>
        <localVar name="locked" dataType="boolean" value="true"/>
        <action template="lock_key"/>
        <action template="lock_pick"/>
    </object>
    <object id="chair_plastic">
        <name>plastic chair</name>
        <component id="chair" type="usable">
            <startPhrase>sit</startPhrase>
            <endPhrase>getUp</endPhrase>
            <startPrompt>Sit</startPrompt>
            <endPrompt>Stand</endPrompt>
        </component>
    </object>
    <object id="counter">
        <name>counter</name>
    </object>
    <object id="gumball_machine">
        <name>gumball machine</name>
        <action template="vending_machine">
            <parameter name="itemID" dataType="string" value="gumball"/>
        </action>
        <networkAction template="vending_machine_hack_price"/>
    </object>
    <object id="garbage_bin">
        <name>garbage bin</name>
        <component id="inventory" type="inventory">
            <inventory>
                <table countMin="0" countMax="3">junk_random</table>
            </inventory>
        </component>
    </object>
    <object id="store_shelf_snacks">
        <name>snacks shelf</name>
        <component id="inventory" type="inventory">
            <inventory>
                <table>store_snacks</table>
            </inventory>
        </component>
    </object>
    <object id="store_shelf_meals">
        <name>meal shelf</name>
        <component id="inventory" type="inventory">
            <inventory>
                <table>store_snacks</table>
            </inventory>
        </component>
    </object>
    <object id="store_shelf_alcohol">
        <name>alcohol shelf</name>
        <component id="inventory" type="inventory">
            <inventory>
                <table>store_snacks</table>
            </inventory>
        </component>
    </object>
    <object id="store_shelf_medicine">
        <name>medicine shelf</name>
        <component id="inventory" type="inventory">
            <inventory>
                <table>store_snacks</table>
            </inventory>
        </component>
    </object>
    <object id="store_cooler_soft_drinks">
        <name>soft drinks cooler</name>
        <component id="inventory" type="inventory">
            <inventory>
                <table>store_soft_drinks</table>
            </inventory>
        </component>
    </object>
    <object id="store_cooler_alcohol">
        <name>alcohol cooler</name>
        <component id="inventory" type="inventory">
            <inventory>
                <table>store_soft_drinks</table>
            </inventory>
        </component>
    </object>
    <object id="vending_machine_nitron">
        <name>Nitron Cola vending machine</name>
        <action template="vending_machine">
            <parameter name="itemID" value="nitron_cola"/>
        </action>
        <action template="vending_machine">
            <parameter name="itemID" value="nitron_diet"/>
        </action>
    </object>
    <object id="motorcycle">
        <name>motorcycle</name>
        <component id="controller" type="vehicle" restricted="true" vehicleType="motorcycle">
            <moveMenuName>Drive</moveMenuName>
        </component>
        <component id="seat" type="usable">
            <startPhrase>sit</startPhrase>
            <endPhrase>getUp</endPhrase>
            <startPrompt>Sit</startPrompt>
            <endPrompt>Stand</endPrompt>
            <exposedComponent>controller</exposedComponent>
        </component>
    </object>
    <object id="network_port">
        <name>network port</name>
        <component id="port" type="usable">
            <startPhrase>jackIn</startPhrase>
            <endPhrase>jackOut</endPhrase>
            <startPrompt>Jack In</startPrompt>
            <endPrompt>Jack Out</endPrompt>
            <exposedComponent>network</exposedComponent>
        </component>
        <component id="network" type="network" restricted="true">
            <networkID type="stat" dataType="string" holder="parentObject" stat="networkID"/>
        </component>
    </object>
</data>