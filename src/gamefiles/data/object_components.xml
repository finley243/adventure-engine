<data>
    <objectComponent id="chair" type="usable">
        <startPhrase>sit</startPhrase>
        <endPhrase>getUp</endPhrase>
        <startPrompt>Sit</startPrompt>
        <endPrompt>Stand</endPrompt>
    </objectComponent>
    <objectComponent id="door_link" type="link" movable="true" visible="false"/>
    <objectComponent id="door_link_locked" type="link" movable="true" visible="false">
        <condition type="boolean">
            <var type="stat" dataType="boolean" stat="succeeded" holder="parentObjectComponent" holderLocalID="door_lock"/>
        </condition>
    </objectComponent>
    <objectComponent id="door_lock_easy" type="check" canFail="false">
        <prompt>Pick lock</prompt>
        <phraseSuccess>pickLock</phraseSuccess>
        <condition type="compare">
            <var1 type="stat" dataType="int" stat="lockpick" holder="subject"/>
            <var2 dataType="int" value="3"/>
        </condition>
    </objectComponent>
    <objectComponent id="garbage_bin_container" type="container">
        <inventory>
            <table countMin="0" countMax="3">junk_random</table>
        </inventory>
    </objectComponent>
    <objectComponent id="store_shelf_snacks_container" type="container">
        <inventory>
            <table>store_snacks</table>
        </inventory>
    </objectComponent>
    <objectComponent id="store_cooler_soft_drinks_container" type="container">
        <inventory>
            <table>store_soft_drinks</table>
        </inventory>
    </objectComponent>
</data>