<data>
    <action id="move_basic">
        <prompt>$areaName ($dir)</prompt>
        <parameter name="areaName" dataType="string" type="stat" holder="area" stat="name">
            <holderID dataType="string" type="parameter" name="areaID"/>
        </parameter>
        <script>
            <script type="sensoryEvent">
                <parameter name="area" dataType="noun" type="stat" holder="area">
                    <holderID dataType="string" type="parameter" name="areaID"/>
                </parameter>
                <phrase>moveTo</phrase>
                <area type="buildStringSet">
                    <value type="stat" dataType="string" holder="subject" subType="area" stat="id"/>
                    <value type="parameter" dataType="string" name="areaID"/>
                </area>
            </script>
            <script type="setState" state="area" holder="subject">
                <value dataType="string" type="parameter" name="areaID"/>
            </script>
        </script>
    </action>
    <action id="move_corner">
        <prompt>Around corner ($dir)</prompt>
        <script>
            <script type="sensoryEvent">
                <parameter name="area" dataType="noun" type="stat" holder="area">
                    <holderID dataType="string" type="parameter" name="areaID"/>
                </parameter>
                <phrase>moveCorner</phrase>
                <area type="buildStringSet">
                    <value type="stat" dataType="string" holder="subject" subType="area" stat="id"/>
                    <value type="parameter" dataType="string" name="areaID"/>
                </area>
            </script>
            <script type="setState" state="area" holder="subject">
                <value dataType="string" type="parameter" name="areaID"/>
            </script>
        </script>
    </action>
</data>